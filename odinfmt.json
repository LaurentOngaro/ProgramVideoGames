{
  "$schema": "https://raw.githubusercontent.com/DanielGavin/ols/master/misc/odinfmt.schema.json",
  "character_width": 150,
  "sort_imports": true,
  "spaces": 2,
  "newline_limit": 1,
  "tabs": false,
  "tabs_width": 2,
  "convert_do": false,
  "exp_multiline_composite_literals": false,
  "brace_style": "_1TBS",
  "indent_cases": true,
